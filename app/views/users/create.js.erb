$("ul.errors").html("")

<% if @user.errors.any? %>
  <% @user.errors.full_messages.each do |e| %>
    $(".errors").append("<p> <%= e %> <p/>")
  <% end %>
<% else %>
  $("#user-modal").modal("hide")
<% end %>
