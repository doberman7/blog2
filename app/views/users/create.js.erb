$("ul.errors").html("")

<% if @user.errors.any? %>
  <% @user.errors.full_messages.each do |e| %>
    $(".errors").append("<li> <%= e %> </li>")
  <% end %>
<% else %>
  // $("#messages-modal").html("<%= escape_javascript(render 'messages-modal') %>");

  $("#user-modal").modal("messages-modal");
  $("#user-modal").modal("hide")
<% end %>
